<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="cn.sunibas.entity">

	<class name="TSkid" table="T_TSKID">
		<id name="id" column="Id" type="java.lang.String" length="32">
		</id>
		<property name="name" column="TSname" type="java.lang.String" length="20"></property>
		<property name="pwd" column="Pwd" type="java.lang.String" length="32"></property>
		<property name="createTime" column="CreateTime" type="java.util.Date"></property>
		<property name="checked" column="Checked" type="java.lang.Integer"></property>
		<property name="role" column="TSrole" type="java.lang.Integer"></property>
	</class>

</hibernate-mapping>



